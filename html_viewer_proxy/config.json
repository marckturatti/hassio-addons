{
  "name": "HTML Viewer Proxy",
  "version": "1.0.0",
  "slug": "html_viewer_proxy",
  "description": "Proxy di un servizio web remoto tramite Nginx",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "80/tcp": 8080
  },
  "webui": "http://[HOST]:[PORT:80]",
  "ingress": true,
  "panel_icon": "mdi:web",
  "panel_title": "HTML Viewer",
  "options": {
    "target_url": "http://localhost:8080"
  },
  "schema": {
    "target_url": "str"
  }
}

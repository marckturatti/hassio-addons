{
  "name": "Web App Viewer",
  "version": "1.0",
  "slug": "web_app_viewer",
  "description": "Visualizza una web app tramite Nginx",
  "startup": "application",
  "boot": "auto",
  "options": {
    "server_url": "http://localhost",
    "server_port": 8080
  },
  "schema": {
    "server_url": "str",
    "server_port": "int"
  },
  "host_network": true,
  "map": ["config:rw"],
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ]
}

